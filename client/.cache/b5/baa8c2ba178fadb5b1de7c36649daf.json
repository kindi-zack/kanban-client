umn":14}},{"source":"src/views/Login.vue","name":null,"original":{"line":62,"column":0},"generated":{"line":73,"column":23}},{"source":"src/views/Login.vue","name":null,"original":{"line":62,"column":0},"generated":{"line":73,"column":26}},{"source":"src/views/Login.vue","name":null,"original":{"line":62,"column":0},"generated":{"line":73,"column":28}},{"source":"src/views/Login.vue","name":null,"original":{"line":63,"column":0},"generated":{"line":74,"column":0}},{"source":"src/views/Login.vue","name":null,"original":{"line":38,"column":0},"generated":{"line":74,"column":7}},{"source":"src/views/Login.vue","name":null,"original":{"line":64,"column":0},"generated":{"line":75,"column":0}},{"source":"src/views/Login.vue","name":null,"original":{"line":36,"column":0},"generated":{"line":76,"column":0}},{"source":"src/views/Login.vue","name":null,"original":{"line":28,"column":0},"generated":{"line":77,"column":0}}],"sources":{"src/views/Login.vue":"<template>\n    <div class=\"container login-container\">\n        <div class=\"row\">\n            <div class=\"col-md-6 login-form-2 bg-secondary\">\n                <h3>Login</h3>\n                <form @submit.prevent=\"login\">\n                    <div class=\"form-group\">\n                        <input type=\"text\" v-model=\"loginEmail\" class=\"form-control\" placeholder=\"Your Email *\" value=\"\" />\n                    </div>\n                    <div class=\"form-group\">\n                        <input type=\"password\" v-model=\"loginPswd\" class=\"form-control\" placeholder=\"Your Password *\" value=\"\" />\n                    </div>\n                    <div class=\"form-group\">\n                        <input type=\"submit\" class=\"btnSubmit btn btn-dark\" value=\"Login\" />\n                    </div>\n                    <div class=\"form-group\">\n                        <a @click.prevent=\"changeDisplayReg('') || changeDisplayLog('d-none')\" href=\"#\" class=\"\" value=\"Login\">Don't have account? Sign up here</a>\n                    </div>\n                </form>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport Swal from 'sweetalert2'\nexport default {\n    props: ['baseUrl', 'changeIsLogin', 'changeDisplayReg', 'changeDisplayLog'],\n    data () {\n        return {\n            loginEmail : '',\n            loginPswd : ''\n        }\n    },\n    methods: {\n        login(){\n            axios({\n                method : 'POST',\n                url : `${this.baseUrl}/login`,\n                data : {\n                    email: this.loginEmail,\n                    password: this.loginPswd\n                }\n\n            })\n            .then(({data})=>{\n                console.log(data.access_token)\n                localStorage.setItem(\"access_token\", data.access_token)\n                this.changeIsLogin(true)\n            })\n            .catch((err)=>{\n                // console.log('errorr <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<')\n                console.log(err.response.data)\n                Swal.fire({\n                    icon: 'error',\n                    text: err.response.data.msg,\n                })\n            })\n            .then(_=>{\n                this.loginEmail = ''\n                this.loginPswd = ''\n            })\n        }\n    }\n}\n</script>\n\n<style>\n.login-container{\n    margin-top: 5%;\n    margin-bottom: 5%;\n    /* height: 100vh; */\n    /* background-color: black */\n}\n\n.row{\n    justify-content: center;\n}\n\n.login-form-2{\n    padding: 5%;\n    /* background: #0062cc; */\n    box-shadow: 0 5px 8px 0 rgba(0, 0, 0, 0.2), 0 9px 26px 0 rgba(0, 0, 0, 0.19);\n}\n.login-form-2 h3{\n    text-align: center;\n    /* color: #fff; */\n}\n.login-container form{\n    padding: 10%;\n}\n.btnSubmit\n{\n    width: 50%;\n    border-radius: 1rem;\n    padding: 1.5%;\n    border: none;\n    cursor: pointer;\n}\n\n.login-form-2 .btnSubmit{\n    font-weight: 600;\n    /* color: #0062cc;\n    background-color: #fff; */\n}\n.login-form-2 .ForgetPwd{\n    /* color: #fff; */\n    font-weight: 600;\n    text-decoration: none;\n}\n\n</style>"},"lineCount":null}},"error":null,"hash":"0543dd7f92670f2c3d2af276ecfe98bb","cacheData":{"env":{}}}